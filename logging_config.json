{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default": {
            "format": "[%(levelname)s] %(asctime)s %(name)s:  %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "formatter": "default",
            "level": "DEBUG",
            "filename": "Logs/awcr_system.log",
            "encoding": "utf-8"
        }
    },
    "root": {
        "handlers": ["file"],
        "level": "DEBUG"
    }
}
